<mvc:View
	controllerName="sap.ui.demo.cart.view.Welcome"
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc" >
	<Page
		title="{i18n>WELCOME_TITLE}"
		enableScrolling="false">
		<footer>
			<Toolbar>
			</Toolbar>
		</footer>
		<content>
			<Text
				text="{i18n>WELCOME_TEXT}"
				class="welcomeText" >
			</Text>
			<HBox
				alignItems="Center"
				justifyContent="Center" >
				<core:Icon
					id="icon5"
					class="welcomeIcon"
					src="sap-icon://receipt"
					size="8em" />
			</HBox>
		</content>
	</Page>
</mvc:View>